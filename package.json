    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-cucumber": "^3.0.1",
